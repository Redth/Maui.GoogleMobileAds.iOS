<Project Sdk="Microsoft.NET.Sdk">
    <PropertyGroup>
        <TargetFramework>net8.0-ios</TargetFramework>
        <Nullable>enable</Nullable>
        <ImplicitUsings>true</ImplicitUsings>
        <IsBindingProject>true</IsBindingProject>
    </PropertyGroup>

    <ItemGroup>
        <NativeReference Include="../.build/UserMessagingPlatform.xcframework">
            <Kind>Static</Kind>
            <SmartLink>True</SmartLink>
            <ForceLoad>True</ForceLoad>
            <LinkerFlags>-ObjC</LinkerFlags>
            <!-- Found in Podspec (eg: https://github.com/CocoaPods/Specs/blob/master/Specs/5/9/a/Google-Mobile-Ads-SDK/11.2.0/Google-Mobile-Ads-SDK.podspec.json )-->
            <Frameworks>WebKit</Frameworks>
            <WeakFrameworks>AdSupport</WeakFrameworks>
        </NativeReference>
    </ItemGroup>

    <PropertyGroup>
        <PackageId>UserMessagingPlatform.iOS.Binding</PackageId>
    </PropertyGroup>

    <ItemGroup>
        <ObjcBindingApiDefinition Include="ApiDefinition.cs"/>
        <ObjcBindingCoreSource Include="StructsAndEnums.cs"/>
    </ItemGroup>
</Project>
