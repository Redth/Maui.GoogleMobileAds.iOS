<Project>
	<PropertyGroup>
		<PackageOutputPath>$(MSBuildThisFileDirectory)/.build/NuGet/</PackageOutputPath>
		<Authors>Redth</Authors>
		<Owners>Redth</Owners>
		<Copyright>Copyright Â© Redth</Copyright>
		<PackageProjectUrl>https://github.com/redth/Maui.GoogleMobileAds.iOS</PackageProjectUrl>
		<PackageLicenseExpression>MIT</PackageLicenseExpression>
		<RepositoryUrl>https://github.com/redth/Maui.GoogleMobileAds.iOS</RepositoryUrl>
		<AssemblyFileVersion>$(PackageVersion)</AssemblyFileVersion>
		<PublishRepositoryUrl>true</PublishRepositoryUrl>
		<SymbolPackageFormat>snupkg</SymbolPackageFormat>
		<DebugType>portable</DebugType>
	</PropertyGroup>

	<PropertyGroup Condition="'$(GITHUB_ACTIONS)' == 'true' Or '$(TF_BUILD)' == 'true'">
		<ContinuousIntegrationBuild>true</ContinuousIntegrationBuild>
	</PropertyGroup>
</Project>